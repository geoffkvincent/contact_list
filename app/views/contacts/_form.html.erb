<h1> New Contact </h1>
<%= form_for @contact do |f| %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :company %>
  <%= f.text_field :company %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :notes %>
  <%= f.text_field :notes %>
  
  <%= f.submit %>
<% end %>